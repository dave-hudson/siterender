{
    "name": "siterender",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "jest": {
        "preset": "ts-jest",
        "transform": {
            "^.+\\.(ts)$": "ts-jest"
        },
        "testEnvironment": "node",
        "testMatch": [
            "**/src/**/*.test.ts"
        ],
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/src/**/*.ts",
            "!**/src/**/test.ts",
            "!**/src/**/*.test.ts"
        ],
        "coverageDirectory": "coverage",
        "coverageReporters": [
            "html",
            "text"
        ]
    },
    "dependencies": {
        "axios": "^1.7.2",
        "fast-xml-parser": "^4.4.1",
        "jest": "^29.7.0",
        "puppeteer": "^22.14.0",
        "ts-jest": "^29.2.3"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12"
    }
}
